// Verifica se o navegador suporta a API de armazenamento local
if (typeof(Storage) !== "undefined") {
  // Obtém o número de itens armazenados no armazenamento local
  var numJsons = Object.keys(localStorage).length;
  console.log("Existem " + numJsons + " arquivos JSON armazenados no navegador.");
} else {
  console.log("O seu navegador não suporta a API de armazenamento local.");
}


localStorage.clear();